{
    "version": 2,
    "name": "timwarren.dev",
    "builds": [
        { "src": "/api/**.js", "use": "@now/node"},
        { "src": "/dist/**", "use": "@now/node"}
    ],
    "routes": [
        { "src": "/", "dest": "/dist/index.html" },
        { "src": "/(.*)", "dest": "/dist/$1" },
        { "src": "/api/*", "dest": "/api/$1.js" }
    ],
    "cleanUrls": true
}
